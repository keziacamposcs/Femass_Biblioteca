<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="professoranchorPane" prefHeight="400.0" prefWidth="352.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bib.Controller.ProfessorController">
   <children>
      <TableView fx:id="TableProfessor" layoutX="14.0" layoutY="52.0" onKeyPressed="#On_Key_Pressed_TableProfessor" onMouseClicked="#On_Mouse_Clicked_TableProfessor" prefHeight="296.0" prefWidth="120.0">
        <columns>
          <TableColumn fx:id="formacaoColumn" prefWidth="119.0" text="Formação" />
        </columns>
      </TableView>
      <Button fx:id="btnProfessor_excluir" layoutX="62.0" layoutY="357.0" mnemonicParsing="false" onAction="#btnProfessor_excluir" prefHeight="29.0" prefWidth="72.0" style="-fx-background-color: #EF476F;" text="Excluir" textFill="WHITE">
         <font>
            <Font name="Avenir Roman" size="13.0" />
         </font></Button>
      <GridPane layoutX="144.0" layoutY="54.0" prefHeight="54.0" prefWidth="186.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="89.0" minWidth="10.0" prefWidth="66.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Formação">
               <font>
                  <Font name="Avenir Roman" size="13.0" />
               </font></Label>
             <TextField fx:id="txtFormacao" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label layoutX="14.0" layoutY="14.0" text="Formação de Professores" textFill="#26547c">
         <font>
            <Font name="Avenir Black" size="13.0" />
         </font></Label>
      <Button fx:id="btnProfessor_salvar" layoutX="211.0" layoutY="117.0" mnemonicParsing="false" onAction="#btnProfessor_salvar" style="-fx-background-color: #EF476F;" text="Salvar" textFill="WHITE">
         <font>
            <Font name="Avenir Roman" size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
